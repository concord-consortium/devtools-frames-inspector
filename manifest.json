{
  "manifest_version": 3,
  "name": "Frames Inspector",
  "version": "1.0.0",
  "description": "Chrome DevTools extension for inspecting frames and postMessage communication",
  "devtools_page": "devtools.html",
  "background": {
    "service_worker": "src/background.js"
  },
  "permissions": ["webNavigation", "storage", "scripting"],
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [
    {
      "resources": ["src/injected.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
